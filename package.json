{
  "dependencies": {
    "@create-figma-plugin/ui": "^4.0.3",
    "@create-figma-plugin/utilities": "^4.0.3",
    "preact": ">=10"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.3",
    "@create-figma-plugin/tsconfig": "^4.0.3",
    "@figma/plugin-typings": "1.109.0",
    "tsx": "^4.21.0",
    "typescript": ">=5"
  },
  "scripts": {
    "generate-presets": "tsx scripts/generate-presets.ts",
    "prebuild": "npm run generate-presets",
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "npm run generate-presets && build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "preact-rectangles",
    "name": "Preact Rectangles",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  }
}
