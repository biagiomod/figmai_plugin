{
  "private": true,
  "license": "MIT",
  "author": "Biagio Goetz",
  "dependencies": {
    "@create-figma-plugin/ui": "4.0.3",
    "@create-figma-plugin/utilities": "4.0.3",
    "preact": "10.28.1"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "4.0.3",
    "@create-figma-plugin/tsconfig": "4.0.3",
    "@figma/plugin-typings": "1.109.0",
    "@types/node": "20.14.15",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "scripts": {
    "generate-presets": "tsx scripts/generate-presets.ts",
    "prebuild": "npm run generate-presets",
    "build": "build-figma-plugin --typecheck --minify",
    "postbuild": "node scripts/check-sync-api.js",
    "watch": "npm run generate-presets && build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "1592005586211314002",
    "name": "FigmAI",
    "main": "src/main.ts",
    "ui": "src/ui.tsx",
    "documentAccess": "dynamic-page",
    "networkAccess": {
      "allowedDomains": [
        "https://api.openai.com",
        "https://overobedient-buddy-leathern.ngrok-free.dev",
        "http://localhost:8787"
      ],
      "reasoning": "This plugin makes outbound requests to an LLM provider (OpenAI) and to a user-operated proxy/tunnel endpoint (ngrok) used to route requests in environments where direct access may be restricted. Localhost access is required for development and for enterprise workflows that run a local proxy service on the user's machine."
    }
  }
}